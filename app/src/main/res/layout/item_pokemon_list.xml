<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="index"
            type="String" />

        <variable
            name="pokemon"
            type="com.yap.pokedex_android.api.paging.object.PokemonVO" />
    </data>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp50_w360"
        android:layout_margin="@dimen/dp10_w360"
        android:elevation="@dimen/dp5_w360">

        <TextView
            style="@style/TextViewSize20_222222Style"
            android:layout_gravity="center"
            tools:text="sixTale"
            android:text="@{index + pokemon.name}"/>

    </androidx.cardview.widget.CardView>

</layout>

